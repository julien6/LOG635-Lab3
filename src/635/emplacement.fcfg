%start S

S[SEM =?vp] -> VP PP[SEM = ?vp]
VP -> N Pron Vt | N V
N -> 'Tu' |'tu'
Pron -> 'te'
Vt -> 'trouves' 
V -> 'es'
PP[SEM = ?vp] -> Prep Det N[SEM = ?vp]
Prep -> 'dans'
Det -> 'le' | 'la'
N[SEM=<Cuisine>] -> 'cuisine'
N[SEM=<Salon>] -> 'salon'
N[SEM=<Burean>] -> 'bureau'
N[SEM=<Garage>] -> 'garage'
N[SEM=<Toilette>] -> 'toilette'

